```elixir
defmodule WordSearch do
  @direction_map %{
    {0, 1} => :right,
    {1, 0} => :down,
    {0, -1} => :left,
    {-1, 0} => :up,
    {1, 1} => :down_right,
    {-1, -1} => :up_left,
    {1, -1} => :down_left,
    {-1, 1} => :up_right
  }

  def find_word(grid, word) do
    directions = Map.to_list(@direction_map)

    for(
      row <- 0..(length(grid) - 1),
      col <- 0..(length(hd(grid)) - 1),
      {{dr, dc}, _} <- directions,
      found? = match_word?(grid, word, row, col, dr, dc),
      found?,
      do: {row, col, @direction_map[{dr, dc}]}
    )

    # |> length()
  end

  defp match_word?(grid, word, row, col, dr, dc) do
    word
    |> String.graphemes()
    |> Enum.with_index()
    |> Enum.all?(fn {char, i} ->
      r = row + dr * i
      c = col + dc * i

      in_bounds?(grid, r, c) and Enum.at(Enum.at(grid, r), c) == char
    end)
  end

  defp in_bounds?(grid, r, c) do
    r >= 0 and r < length(grid) and c >= 0 and c < length(hd(grid))
  end
end
```

```elixir
